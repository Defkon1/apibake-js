{
  "name": "apibake",
  "version": "0.1.23",
  "description": "Convert OpenAPI spec to PDF.",
  "keywords": [
    "OpenAPI",
    "Open API",
    "Swagger",
    "REST API",
    "API spec",
    "PDF",
    "generator",
    "PDF generator",
    "doc generator",
    "API spec generator",
    "API doc generator"
  ],
  "author": "Illya Sikeryn",
  "license": "MIT",
  "homepage": "https://curvednebula.com/apibake/",
  "repository": {
    "type": "git",
    "url": "https://github.com/curvednebula/apibake-js"
  },
  "main": "index.js",
  "bin": {
    "apibake": "./dist/index.js"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "new-ver": "npm version patch && npm run build && git commit -am 'new version' && git push origin",
    "publish-to-npm": "npm login && npm publish",
    "dev": "ts-node ./src/index.ts test-data/v3.0 test-data/v3.1 test-data/private --title 'REST API Spec' --subtitle 'Created with ApiBake' --separate-schemas",
    "test": "npx apibake@latest test-data/v3.0 test-data/v3.1 test-data/private --title 'REST API Spec' --subtitle 'Created with ApiBake'",
    "pdfkit-test": "ts-node ./src/test/pdf-kit-tests.ts",
    "prod": "node ./dist/index.js"
  },
  "devDependencies": {
    "@types/node": "^20.5.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "moment": "^2.29.4",
    "pdfkit": "^0.13.0",
    "yaml": "^2.3.2"
  }
}
